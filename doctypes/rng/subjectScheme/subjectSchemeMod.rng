        <optional>
          <attribute name="status">
            <choice>
              <value>changed</value>
              <value>deleted</value>
              <value>new</value>
              <value>unchanged</value>
              <value>-dita-use-conref-target</value>
            </choice>
          </attribute>
        </optional>
        <optional>
          <attribute name="base"/>
        </optional>
