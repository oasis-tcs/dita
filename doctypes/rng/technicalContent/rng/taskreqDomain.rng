      <a:documentation> specialized from ol </a:documentation>
      <define name="safety.content">
        <zeroOrMore dita:since="1.3">
          <choice>
            <ref name="data"/>
            <ref name="data-about"/>
          </choice>
        </zeroOrMore>
        <choice>
          <ref name="nosafety"/>
