            <ref name="lcFeedback.attlist"/>
            <ref name="lcFeedback.content"/>
          </element>
        </define>
        <define name="lcFeedback.attlist" combine="interleave">
          <ref name="lcFeedback.attributes"/>
        </define>
      </div>
