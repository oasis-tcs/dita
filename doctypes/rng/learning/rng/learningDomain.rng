  <div>
    <a:documentation>ELEMENT TYPE DECLARATIONS</a:documentation>

    <div>
      <a:documentation>MISCELANEOUS TYPES</a:documentation>
      <div>
        <a:documentation>LONG NAME: Instructor note</a:documentation>
        <define name="lcInstructornote.content">
          <zeroOrMore>
            <ref name="note.cnt"/>
          </zeroOrMore>
        </define>
        <define name="lcInstructornote.attributes">
          <optional>
            <attribute name="spectitle"/>
          </optional>
          <ref name="univ-atts"/>
          <optional>
            <attribute name="outputclass"/>
          </optional>
        </define>
        <define name="lcInstructornote.element">
          <element name="lcInstructornote" dita:longName="Instructor note">
            <a:documentation><![CDATA[Use the <lcInstructornote> element to provide information or notes you want to provide to the course instructor. These notes can be conditionnalized out of content you intend to deliver to the learner. ]]></a:documentation>
            <ref name="lcInstructornote.attlist"/>
            <ref name="lcInstructornote.content"/>
          </element>
        </define>
        <define name="lcInstructornote.attlist" combine="interleave">
          <ref name="lcInstructornote.attributes"/>
        </define>
      </div>
    </div>
    <div>
      <a:documentation>INTERACTION DEFINITIONS</a:documentation>
      <div>
        <define name="lcTrueFalse.content">
          <optional>
            <ref name="title"/>
          </optional>
          <ref name="lcQuestion"/>
          <optional>
            <ref name="lcAsset"/>
          </optional>
          <ref name="lcAnswerOptionGroup"/>
          <optional>
            <ref name="lcFeedbackIncorrect"/>
          </optional>
          <optional>
            <ref name="lcFeedbackCorrect"/>
          </optional>
          <zeroOrMore>
            <ref name="data"/>
          </zeroOrMore>
        </define>
        <define name="lcTrueFalse.attributes">
          <attribute name="id">
            <data type="NMTOKEN"/>
          </attribute>
          <ref name="conref-atts"/>
          <ref name="select-atts"/>
          <ref name="localization-atts"/>
          <optional>
            <attribute name="outputclass"/>
          </optional>
        </define>
        <define name="lcTrueFalse.element">
          <element name="lcTrueFalse" dita:longName="True or false interaction">
            <a:documentation><![CDATA[A lcTrueFalse interaction presents the learner with two choices, one correct, the other incorrect, often presented as true/false or yes/no responses. ]]></a:documentation>
            <ref name="lcTrueFalse.attlist"/>
            <ref name="lcTrueFalse.content"/>
          </element>
        </define>
        <define name="lcTrueFalse.attlist" combine="interleave">
          <ref name="lcTrueFalse.attributes"/>
        </define>
      </div>

      <div>
        <define name="lcSingleSelect.content">
