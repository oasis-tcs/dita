    <div>
      <a:documentation>OPTION DEFINITIONS</a:documentation>
      <div>
        <define name="lcQuestion.content">
          <zeroOrMore>
            <ref name="ph.cnt"/>
          </zeroOrMore>
        </define>
        <define name="lcQuestion.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="outputclass"/>
          </optional>
        </define>
        <define name="lcQuestion.element">
          <element name="lcQuestion" dita:longName="Question (prompt)">
            <a:documentation><![CDATA[Use the <lcQuestion> element in an interaction to ask the question. ]]></a:documentation>
            <ref name="lcQuestion.attlist"/>
            <ref name="lcQuestion.content"/>
          </element>
        </define>
        <define name="lcQuestion.attlist" combine="interleave">
          <ref name="lcQuestion.attributes"/>
        </define>
      </div>
      <div>
        <define name="lcOpenAnswer.content">
          <zeroOrMore>
            <ref name="ph.cnt"/>
          </zeroOrMore>
        </define>
        <define name="lcOpenAnswer.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="outputclass"/>
          </optional>
        </define>
        <define name="lcOpenAnswer.element">
          <element name="lcOpenAnswer" dita:longName="Open answer">
            <a:documentation><![CDATA[Use <lcOpenAnswer> to provide a suggested answer for an <lcOpenQuestion> interaction. ]]></a:documentation>
            <ref name="lcOpenAnswer.attlist"/>
            <ref name="lcOpenAnswer.content"/>
          </element>
        </define>
        <define name="lcOpenAnswer.attlist" combine="interleave">
          <ref name="lcOpenAnswer.attributes"/>
        </define>
      </div>
      <div>
        <define name="lcAnswerOptionGroup.content">
          <zeroOrMore dita:since="1.3">
            <choice>
              <ref name="data"/>
              <ref name="data-about"/>
            </choice>
          </zeroOrMore>
          <oneOrMore>
            <ref name="lcAnswerOption"/>
          </oneOrMore>
        </define>
        <define name="lcAnswerOptionGroup.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="outputclass"/>
          </optional>
        </define>
        <define name="lcAnswerOptionGroup.element">
          <a:documentation><![CDATA[The <lcAnswerOptionGroup> element provides a container for the options for a true-false, single-select, or multiple-select assessment interaction. ]]></a:documentation>
          <element name="lcAnswerOptionGroup" dita:longName="Answer option group">
            <ref name="lcAnswerOptionGroup.attlist"/>
            <ref name="lcAnswerOptionGroup.content"/>
          </element>
        </define>
        <define name="lcAnswerOptionGroup.attlist" combine="interleave">
          <ref name="lcAnswerOptionGroup.attributes"/>
        </define>
      </div>
      <div>
        <define name="lcSequenceOptionGroup.content">
          <zeroOrMore dita:since="1.3">
            <choice>
              <ref name="data"/>
              <ref name="data-about"/>
            </choice>
          </zeroOrMore>
          <oneOrMore>
            <ref name="lcSequenceOption"/>
          </oneOrMore>
        </define>
        <define name="lcSequenceOptionGroup.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="outputclass"/>
          </optional>
        </define>
        <define name="lcSequenceOptionGroup.element">
          <element name="lcSequenceOptionGroup" dita:longName="Sequence option group">
            <a:documentation><![CDATA[The <lcSequenceOptionGroup> element provides the options for an assessment sequence interaction. ]]></a:documentation>
            <ref name="lcSequenceOptionGroup.attlist"/>
            <ref name="lcSequenceOptionGroup.content"/>
          </element>
        </define>
        <define name="lcSequenceOptionGroup.attlist" combine="interleave">
          <ref name="lcSequenceOptionGroup.attributes"/>
        </define>
      </div>
      <div>
        <define name="lcAsset.content">
          <zeroOrMore>
            <choice>
              <ref name="imagemap"/>
              <ref name="image"/>
              <ref name="object"/>
            </choice>
          </zeroOrMore>
        </define>
        <define name="lcAsset.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="outputclass"/>
          </optional>
        </define>
        <define name="lcAsset.element">
          <element name="lcAsset" dita:longName="Interaction asset">
            <a:documentation><![CDATA[The <lcAsset> element in an assessment interaction provides the images or other graphic assets to support the interaction. ]]></a:documentation>
            <ref name="lcAsset.attlist"/>
            <ref name="lcAsset.content"/>
          </element>
        </define>
        <define name="lcAsset.attlist" combine="interleave">
          <ref name="lcAsset.attributes"/>
        </define>
      </div>
      <div>
        <define name="lcSequenceOption.content">
          <ref name="lcAnswerContent"/>
          <ref name="lcSequence"/>
        </define>
        <define name="lcSequenceOption.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="outputclass"/>
          </optional>
        </define>
        <define name="lcSequenceOption.element">
          <element name="lcSequenceOption" dita:longName="Sequence option">
            <a:documentation><![CDATA[The <lcSequenceOption> element in an assessment interaction provides the contents of an item in a sequence interaction. ]]></a:documentation>
            <ref name="lcSequenceOption.attlist"/>
            <ref name="lcSequenceOption.content"/>
          </element>
        </define>
        <define name="lcSequenceOption.attlist" combine="interleave">
          <ref name="lcSequenceOption.attributes"/>
        </define>
      </div>
      <div>
        <define name="lcFeedback.content">
          <zeroOrMore>
            <ref name="ph.cnt"/>
          </zeroOrMore>
        </define>
        <define name="lcFeedback.attributes">
          <ref name="univ-atts"/>
