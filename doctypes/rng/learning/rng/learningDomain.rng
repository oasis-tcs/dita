          <ref name="lcHotspot.attributes"/>
        </define>

      </div>
      <div>
        <define name="lcOpenQuestion.content">
          <optional>
            <ref name="title"/>
          </optional>
          <ref name="lcQuestion"/>
          <optional>
            <ref name="lcAsset"/>
          </optional>
          <optional>
            <ref name="lcOpenAnswer"/>
          </optional>
          <optional>
            <ref name="lcFeedbackIncorrect"/>
          </optional>
          <optional>
            <ref name="lcFeedbackCorrect"/>
