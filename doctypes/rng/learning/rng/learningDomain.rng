          <optional>
            <attribute name="outputclass"/>
          </optional>
        </define>
        <define name="lcFeedbackCorrect.element">
          <element name="lcFeedbackCorrect" dita:longName="Correct-response feedback">
            <a:documentation><![CDATA[The <lcFeedbackCorrect> element in an assessment interaction provides feedback to the learner about a correct response. ]]></a:documentation>
            <ref name="lcFeedbackCorrect.attlist"/>
