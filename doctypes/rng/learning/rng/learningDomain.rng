        </define>
      </div>
      <div>
        <define name="lcAnswerOption.content">
          <ref name="lcAnswerContent"/>
          <optional>
            <ref name="lcCorrectResponse"/>
          </optional>
          <optional>
            <ref name="lcFeedback"/>
          </optional>
        </define>
        <define name="lcAnswerOption.attributes">
          <ref name="univ-atts"/>
          <optional>
            <attribute name="outputclass"/>
