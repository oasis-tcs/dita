      <fig class="- topic/fig " id="fig_BF890E35527C41FBA93878660EFC1636" xtrc="fig:5;135:54" xtrf="file:/C:/Git/dita/specification/langRef/attributes/theconrefendattribute.dita">
        <title class="- topic/title " xtrc="title:9;136:16" xtrf="file:/C:/Git/dita/specification/langRef/attributes/theconrefendattribute.dita">Block level example: Reusing topic with conrefs</title>
        <codeblock class="+ topic/pre pr-d/codeblock " xml:space="preserve" xtrc="codeblock:5;137:20" xtrf="file:/C:/Git/dita/specification/langRef/attributes/theconrefendattribute.dita"> &lt;topic id="y"&gt;
 	...
 	&lt;body&gt;
