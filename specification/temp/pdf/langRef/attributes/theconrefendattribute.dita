        <codeblock class="+ topic/pre pr-d/codeblock " xml:space="preserve" xtrc="codeblock:3;100:20" xtrf="file:/C:/Git/dita/specification/langRef/attributes/theconrefendattribute.dita"> &lt;topic id="y"&gt;
