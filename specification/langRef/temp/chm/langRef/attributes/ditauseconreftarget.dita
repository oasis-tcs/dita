        <codeblock xml:space="preserve" class="+ topic/pre pr-d/codeblock " xtrf="file:/C:/Git/dita/specification/langRef/attributes/ditauseconreftarget.dita" xtrc="codeblock:2;60:21">&lt;map&gt;&lt;title&gt;Conref demonstration&lt;/title&gt;
  &lt;topichead id="heading"
             navtitle="This is a heading"
             toc="yes"
             linking="normal"&gt;
