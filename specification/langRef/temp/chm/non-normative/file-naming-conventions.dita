        <stentry class="- topic/stentry " xtrf="file:/C:/Git/dita/specification/non-normative/file-naming-conventions.dita" xtrc="stentry:51;201:18"><filepath class="+ topic/ph sw-d/filepath " xtrf="file:/C:/Git/dita/specification/non-normative/file-naming-conventions.dita" xtrc="filepath:32;201:28"><varname class="+ topic/keyword sw-d/varname " xtrf="file:/C:/Git/dita/specification/non-normative/file-naming-conventions.dita" xtrc="varname:29;201:37">qualifier</varname><varname class="+ topic/keyword sw-d/varname " xtrf="file:/C:/Git/dita/specification/non-normative/file-naming-conventions.dita" xtrc="varname:30;201:65">domain</varname>DomainConstraint.ent</filepath></stentry>
